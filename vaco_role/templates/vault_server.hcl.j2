listener "tcp" {
  address          = "0.0.0.0:8200"
  cluster_address  = "{{ vault_private_ip[0] }}:8201"
  tls_disable      = "true"
}

storage "consul" {
  address = "127.0.0.1:8500"
  path    = "vault/"
}

api_addr = "http://{{ vault_private_ip[0] }}:8200"
cluster_addr = "https://{{ vault_private_ip[0] }}:8201"
ui = true
